<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyAppTheme">
        <attr name="colorPrimary" format="color"/>
        <attr name="colorPrimaryDark" format="color"/>
        <attr name="colorAccent" format="color"/>
        <attr name="colorText" format="color"/>
        <attr name="colorButtonText" format="color"/>
        <attr name="colorIcons" format="color"/>
        <attr name="colorMenuIcons" format="color"/>
        <attr name="colorBackground" format="color"/>
        <attr name="colorListItemBackground" format="color"/>
        <attr name="colorControlLight" format="color"/>
        <attr name="colorIndicatorInactive" format="color" />
        <attr name="eventFilterViewStyle" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SkillView">
        <attr name="android:checked"/>
        <attr name="android:text"/>
        <attr name="android:textColor"/>
        <attr name="android:textSize"/>
        <attr name="android:color"/>
        <attr name="android:padding"/>
        <attr name="android:strokeColor"/>
        <attr name="android:letterSpacing"/>
        <attr name="outlineWidth" format="dimension"/>
        <attr name="clearIcon" format="reference"/>
        <attr name="foreground" format="reference"/>
        <attr name="showIcons" format="boolean"/>
        <attr name="cornerRadius" format="dimension"/>
    </declare-styleable>
